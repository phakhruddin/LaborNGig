<Alloy>
<Collection src="project"/>
 <Tab id="tab_project" title="project">
  <Window id="project_window" backgroundImage="bg-red1.png" title="project List">
	  <TableView id="project_table" backgroundColor="transparent" dataCollection="project" search="search_history" dataTransform="transformFunction" filterAttribute="title">
		<SearchBar id="search_history" height="43" />
		  <TableViewRow id="project_row" backgroundColor="transparent" opacity="0"  color="transparent" title="{title}">
		  	<Label color="#D8D8BF" textAlign="Ti.UI.TEXT_ALIGNMENT_CENTER" text="{custom}"/>
		  </TableViewRow>
	  </TableView>
  </Window>
  </Tab>
</Alloy>