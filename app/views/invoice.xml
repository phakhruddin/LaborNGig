<Alloy>
	<Collection src="invoice"/>
 <Tab id="tab_invoice" title="invoice">
  <Window id="invoice_window" backgroundImage="blblue-bg1.png"  title="Invoice List">
	  <TableView id="invoice_table" backgroundColor="transparent" dataCollection="invoice" search="search_history" dataTransform="transformFunction" filterAttribute="title">
		<SearchBar id="search_history" height="43" />
		  <TableViewRow id="invoice_row" backgroundColor="transparent" opacity="0"  color="transparent" title="{title}">
		  	<Label color="#D8D8BF" textAlign="Ti.UI.TEXT_ALIGNMENT_CENTER" text="{custom}"/>
		  </TableViewRow>
	  </TableView>
  </Window>
    </Tab>
</Alloy>