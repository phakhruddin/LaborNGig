<Alloy>
	<Model src="dummy" />
  <Window id="clientdetail_window" title="Client Detail" backgroundImage="lngembossi6page4.png" backgroundColor="transparent">
	  <TableView id="clientdetail_table"  backgroundColor="transparent">
	  	<TableViewSection headerTitle="ContactDetails">
		  <TableViewRow id="clientdetail_row" Title="ClientDetail">
		  	  <Label id="FullName" text="{dummy.fullname}" />
		  </TableViewRow>
		  <TableViewRow id="phoneclientdetail_row" Title="PhoneClientDetail">
	   		  <Label id="phone" text="{dummy.phone}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="addressclientdetail_row" Title="addressClientDetail">
	   		  <Label id="address" text="{dummy.address}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="emailclientdetail_row" Title="emailClientDetail">
	   		  <Label id="email" text="{dummy.email}" />
	   	  </TableViewRow>
	   	  <TableViewRow id="companyclientdetail_row" Title="companyClientDetail">
	   		  <Label id="company" text="{dummy.company}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Pending Invoice">
		  <TableViewRow id="invoiceclientdetail_row" Title="invoiceClientDetail">
	   		  <Label id="invoice" text="{dummy.invoice}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Pending Project">
		  <TableViewRow id="projectclientdetail_row" Title="projectClientDetail">
	   		  <Label id="project" text="{dummy.project}" />
		  </TableViewRow>
	  </TableViewSection>
	  <TableViewSection headerTitle="Pending Proposal">
		  <TableViewRow id="proposalclientdetail_row" Title="proposalClientDetail">
	   		  <Label id="proposal" text="{dummy.proposal}" />
		  </TableViewRow>
	  </TableViewSection>
	  </TableView>
  </Window>
</Alloy>